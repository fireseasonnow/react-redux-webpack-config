{
    "parser": "babel-eslint",
    "env": {
        "jest": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4, {"SwitchCase": 1}],
        "react/jsx-indent": ["error", 4],
        "react/jsx-filename-extension": ["disable"],
        "comma-dangle": ["error", {"objects": "always-multiline"}]
    }
}